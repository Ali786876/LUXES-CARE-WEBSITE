body { font-family: sans-serif; margin: 0; padding: 20px; color: #333; }
header, footer { text-align: center; padding: 10px; background: #f7f7f7; }
main { max-width: 600px; margin: 20px auto; }
img { max-width: 100%; height: auto; display: block; margin: 0 auto; }
button { display: block; margin: 20px auto; padding: 10px 20px; background: #007BFF; color: #fff; border: none; cursor: not-allowed; }
nav a { margin: 0 5px; color: #007BFF; text-decoration: none; }
